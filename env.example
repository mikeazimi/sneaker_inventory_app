# Supabase Configuration
# Copy this file to .env and fill in your values

# Your Supabase project URL
SUPABASE_URL=https://your-project-ref.supabase.co

# Public anonymous key (safe for client-side)
SUPABASE_ANON_KEY=your-anon-key

# Service role key (server-side only, never expose to client!)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# ShipHero OAuth Configuration
SHIPHERO_CLIENT_ID=your-shiphero-client-id
SHIPHERO_CLIENT_SECRET=your-shiphero-client-secret
SHIPHERO_REDIRECT_URI=https://your-project-ref.supabase.co/functions/v1/auth-callback

